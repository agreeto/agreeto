{
  "name": "extension",
  "displayName": "Extension",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "richardpoelderl",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@plasmohq/storage": "^0.12.1",
    "@tanstack/react-query": "^4.7.2",
    "next-auth": "^4.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.2",
    "@types/chrome": "0.0.197",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "plasmo": "0.55.0",
    "prettier": "2.7.1",
    "ts-to-zod": "^1.13.1",
    "typescript": "4.8.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "storage"
    ],
    "externally_connectable": {
      "matches": [
        "https://*.agreeto.app/*",
        "*://localhost/*"
      ]
    }
  }
}
