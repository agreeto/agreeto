{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", ".expo/**", ".plasmo/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "postinstall": {
      "cache": false
    }
  },
  "globalEnv": ["NODE_ENV", "PLASMO_PUBLIC_WEB_URL", "SKIP_ENV_VALIDATION"]
}
