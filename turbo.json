{
  "pipeline": {
    "db-generate": {
      "cache": false
    },
    "db-push": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db-generate"],
      "outputs": [".next/**", ".expo/**", ".plasmo/**", "build/**", "dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^db-generate", "^build"],
      "outputs": [],
      "cache": false
    }
  },
  "globalEnv": [
    "CI",
    "NODE_ENV",
    "NEXT_PUBLIC_PORT",
    "VERCEL",
    "VERCEL_URL",
    "PLASMO_PUBLIC_WEB_URL",
    "NEXT_PUBLIC_EXTENSION_ID",
    "NEXT_PUBLIC_OUTLOOK_ADDIN_URL",
    "NEXT_PUBLIC_PORT",
    "SKIP_ENV_VALIDATION",
    "NEXTAUTH_SECRET",
    "DATABASE_URL",
    "GOOGLE_ID",
    "GOOGLE_SECRET",
    "AZURE_AD_CLIENT_ID",
    "AZURE_AD_CLIENT_SECRET",
    "AZURE_AD_TENANT_ID",
    "VITE_API_URL"
  ]
}
